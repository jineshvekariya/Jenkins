pipeline{
    agent{
        label 'devops'
    }
    stages{
        stage('App Code Checkout') {
            steps{
                git url: 'https://github.com/jineshvekariya/jenkins.git', branch: 'main'    
            }  
        }

        stage('Build Image') {
            steps{
                sh 'ls -la'
                sh 'docker build -t jenkinsapp:latest .'
            }
        }
    }
}
